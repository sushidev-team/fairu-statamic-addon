{
  "name": "sushidev/fairu-statamic",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "Sushidev\\Fairu\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sushidev\\Fairu\\Tests\\": "tests"
    }
  },
  "require": {
    "statamic/cms": "^6",
    "laravel/framework": "^12.0",
    "laravel/prompts": "^0.3.5"
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "extra": {
    "statamic": {
      "name": "Fairu",
      "description": "Fairu addon"
    },
    "laravel": {
      "providers": [
        "Sushidev\\Fairu\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
